datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        Int      @id @default(autoincrement())
  name      String
  email     String   @unique
  password  String
  createdAt DateTime @default(now())
}

model ProcessedImage {
  id          Int      @id @default(autoincrement())
  customId    String?  
  imagePath   String
  l1          Float
  l2          Float
  l3          Float
  l4          Float
  l5          Float
  w1          Float
  w2          Float
  w3          Float
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
